{
  "name": "app.decenterai.com",
  "version": "0.5.0",
  "description": "Decenter AI Official App: www.app.decenterai.com",
  "author": "laciferin@gmail.com",
  "license": "MIT",
  "private": true,
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "npm run build",
    "dev": "next dev",
    "start:dev": "concurrently \"pnpm:dev\" \"pnpm:prisma:generate:watch\"",
    "prebuild": "npm run prisma:generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "run-cid-archived": "ts-node utils/archived/cid.ts",
    "run-cid": "ts-node utils/cid.ts",
    "run-compute": "ts-node utils/compute.ts",
    "clean": "rimraf .next build",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:test:deploy": "cp .env.test .env && prisma migrate deploy && rm .env",
    "prisma:local:deploy": "cp .env.local .env && prisma migrate dev && rm .env",
    "prisma:deploy": "DEBUG=* prisma migrate deploy",
    "test": "jest --passWithNoTests",
    "test:watch": "MODE=test jest --watch",
    "test:cov": "MODE=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "MODE=test jest --config ./test/jest-e2e.json --passWithNoTests",
    "test:e2e:watch": "MODE=test jest --watch --no-cache --config ./test/jest-e2e.json",
    "style": "prettier -w .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@lighthouse-web3/sdk": "^0.2.8",
    "@prisma/client": "^5.4.2",
    "@rainbow-me/rainbowkit": "^1.1.1",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@wagmi/core": "^1.4.3",
    "@web3auth/base": "^7.0.4",
    "@web3auth/ethereum-provider": "^7.0.4",
    "@web3auth/no-modal": "^7.0.4",
    "@web3auth/openlogin-adapter": "^7.0.4",
    "argon2": "^0.31.1",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.1",
    "blob-to-buffer": "^1.2.9",
    "cid": "github:multiformats/cid",
    "cids": "^1.1.9",
    "encoding": "^0.1.13",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "ethers": "^6.8.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "fs": "0.0.1-security",
    "jszip": "^3.10.1",
    "lokijs": "^1.5.12",
    "multiformats": "^12.1.2",
    "next": "13.4.19",
    "pino-pretty": "^10.2.3",
    "postcss": "8.4.29",
    "random-avatar-generator": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "react-spinners": "^0.13.8",
    "save-as": "^0.1.8",
    "semver": "^7.5.4",
    "sharp": "^0.32.6",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "unique-username-generator": "^1.2.0",
    "viem": "^1.16.0",
    "wagmi": "^1.4.3",
    "web3.storage": "^4.5.5"
  },
  "devDependencies": {
    "@types/blob-to-buffer": "^1.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.5",
    "@types/mocha": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "concurrently": "^8.2.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.12.0",
    "husky": "^8.0.3",
    "is-ipfs": "^8.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prisma": "^5.4.2",
    "react-devtools": "^4.28.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "tls": false
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0",
    "pnpm": ">=8.6.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Decenter-AI/app.decenterai.com.git"
  },
  "bugs": {
    "url": "https://github.com/Decenter-AI/app.decenterai.com/issues"
  },
  "homepage": "https://app.decenterai.com",
  "packageManager": "pnpm@8.6.6",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  }
}
